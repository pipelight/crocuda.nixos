// StartUp
spawn-at-startup "gammastep -P -O 5500 -b 0.8:0.8"
spawn-at-startup "wlr-which-key"


// Inputs
input {
    keyboard {
        xkb {
            layout "us"
        }
        numlock
    }
    touchpad {
        tap
    }
}

// Laptop
output "eDP-1" {
    scale 1.0
}
// Desktop
output "DP-0" {

}

binds {

}

